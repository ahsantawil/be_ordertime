<%- include ../../partials/header.ejs %>
<%- include ../../partials/navbar.ejs %>
<%- include ../../partials/sidebar.ejs %>
  
<div class="content-wrapper">
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0">Data Province</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="/province">Province</a></li>
              <li class="breadcrumb-item active">Home</li>
            </ol>
          </div>
        </div>
      </div>
    </div>
  
    <div class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-lg-12">

            <div class="card card-primary">
                <div class="card-header">
                  <h3 class="card-title">Add Province</h3>
                </div>
            
                <form action="/province/add" method="post">
                  <div class="card-body">
                    <div class="form-group">
                      <label for="nameProvince">Province</label>
                      <input type="text" name="nameProvince" class="form-control" id="nameProvince" placeholder="Enter Province" required>
                    </div>
                  </div>
            
                  <div class="card-footer">
                    <button type="submit" class="btn btn-primary">Save</button>
                  </div>
                </form>
            </div>
            
          </div>
          
        </div>
      
      </div>
     
    </div>

</div>
  
<%- include ../../partials/footer.ejs %>
<%- include ../../partials/js.ejs %>